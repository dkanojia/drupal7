  $query= db_select('node','n'); 
  $query->join('field_data_field_manager_cost_code','mcc','mcc.entity_id = n.nid');
  $query->join('og_membership','om','om.etid=n.nid AND om.entity_type=:entityType', array(':entityType'=>'node'));
