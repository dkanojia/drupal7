  $node->field_schedule_projected['und'][0]['value'] = $formVal['schedule_projected'];
  $node->field_schedule_actual['und'][0]['value'] = $formVal['schedule_actual'];  
  // Status mark work
  if($formVal['status_mark_data_id'] != '') {
