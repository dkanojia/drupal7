  if(!empty($quar) && $quar != '') {  
    if($quar == 1) { $quar = 'firstQuarter'; } 
    else if($quar == 2) { $quar = 'secondQuarter'; }
    else if($quar == 3) { $quar = 'thirdQuarter'; }
    else if($quar == 4) { $quar = 'fourthQuarter'; }
    $currentQuarter = $quar;
  }
  else {
    $currentQuarter = _get_current_quarter(date('n'));  
  }
  
