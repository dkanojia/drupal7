  } 
  
  $node->field_project_reference['und'][0]['nid'] = $formVal['select-schedule'];  
  node_save($node);
  drupal_goto('/program/schedule', array('query'=>array('pid' => $formVal['select-schedule'],'scid' => $node->nid)));
