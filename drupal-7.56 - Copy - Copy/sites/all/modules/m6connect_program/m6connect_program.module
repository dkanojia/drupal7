function get_description_by_cost_code_mod($costCode = 0) {
  if($costCode != '') { 
    $getCcDes = db_select('node','n'); 
    $getCcDes->join('field_data_field_manager_cost_code','mcc','mcc.entity_id = n.nid'); 
