        $getAdditionalImageSrc['makeDefault'][] = '<p><input type="radio" class="make-default-update" value="'.$getFid.'" for="'.$assetNode->nid.'" name="thumb-image"></p>';
      }
    }
    else {
      $getAdditionalImageSrc['makeDefault'][] = '<p><input type="radio" class="make-default-update" value="'.$getFid.'" for="'.$assetNode->nid.'" name="thumb-image"></p>';
    }
    
